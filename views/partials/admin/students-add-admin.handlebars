<h1>ADD STUDENT</h1>
<form action="/insertfaculty" id="addproduct-form" method="POST" role="form">
  <fieldset>
    <div class="form-row" style="margin-bottom: 14px;">
      <div class="col">
        <label for="name">First Name</label>
        <input class="form-control" id="name" name="name" type="text" placeholder="First Name" required>
      </div>
      <div class="col">
        <label for="price">Last Name</label>
        <input class="form-control" id="price" name="price" type="text" placeholder="Last Name" required>
      </div>
    </div>
    <div class="form-row" style="margin-bottom: 14px;">
      <div class="col">
        <label for="warranty">Student Number</label>
        <input class="form-control" id="warranty" name="warranty" type="number" placeholder="Student Number">
      </div>
    </div>
    <div class="form-row" style="margin-bottom: 14px;">
      <div class="col">
        <label for="warranty">E-mail</label>
        <input class="form-control" id="warranty" name="warranty" type="email" placeholder="E-mail">
      </div>
    </div>
    <div class="form-row" style="margin-bottom: 14px;">
      <div class="col">
        <label for="warranty">Phone Number</label>
        <input class="form-control" id="warranty" name="warranty" type="number" placeholder="Phone Number">
      </div>
    </div>
    <div class="form-row" style="margin-bottom: 14px;">
      <div class="col">
        <label for="name">Password</label>
        <input class="form-control" id="tagline" name="tagline" type="password" placeholder="Password"></input>
      </div>
      <div class="col">
        <label for="price">Confirm Password</label>
        <input class="form-control" id="description" name="description" type="password" placeholder="Confirm Password"></input>
      </div>
    </div>
